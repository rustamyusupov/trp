{
  "name": "TRP",
  "description": "Copy TrainerRoad workout to TrainerPeaks",
  "version": "0.1.2",
  "author": "Rustam Yusupov",
  "manifest_version": 3,
  "default_locale": "en",
  "icons": {
    "128": "images/bike128.png",
    "64": "images/bike64.png",
    "32": "images/bike32.png",
    "16": "images/bike16.png"
  },
  "permissions": ["activeTab", "storage", "scripting"],
  "action": {
    "default_popup": "index.html"
  }
}
